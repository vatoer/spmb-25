{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "outputs": []
    },
    "build": {
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "start": {
      "outputs": [],
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "outputs": []
    },
    "check-types": {
      "outputs": []
    }
  }
}